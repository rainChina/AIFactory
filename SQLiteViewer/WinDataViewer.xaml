<Window x:Class="SQLiteViewer.WinDataViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SQLiteViewer" xmlns:lvc="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
        mc:Ignorable="d"
        Title="WinDataViewer" Height="450" Width="800">
    <Grid>
        <lvc:CartesianChart Grid.Row="2" Name="lvcChart"
        Series="{Binding Series}"
        XAxes="{Binding XAxes}">    
        </lvc:CartesianChart>
    </Grid>
</Window>
