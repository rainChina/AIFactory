<hc:Window x:Class="AIFactory.View.WinDigitalScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AIFactory.View" 
        xmlns:hc="https://handyorg.github.io/handycontrol" xmlns:viewmodel="clr-namespace:AIFactory.ViewModel"
        mc:Ignorable="d"
        Title="" Height="450" Width="800" WindowState="Maximized">
    <hc:Window.DataContext>
        <viewmodel:ViewModelChart></viewmodel:ViewModelChart>
    </hc:Window.DataContext>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="0.3*"></RowDefinition>
        </Grid.RowDefinitions>
        <local:ViewChart></local:ViewChart>
        <local:ViewDashboard Grid.Row="1"/>
    </Grid>
</hc:Window>
