<UserControl x:Class="AIFactory.View.ViewChart"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AIFactory.View" xmlns:lvc="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TabControl TabStripPlacement="Bottom" Margin="0">
            <TabItem Header="Title" HeaderStringFormat="{}{0}1">
                <lvc:CartesianChart Series="{Binding Series}" Title="{Binding LTitle}"/>
            </TabItem>
            <TabItem Header="Title" HeaderStringFormat="{}{0}2" IsEnabled="False">
                <lvc:CartesianChart Series="{Binding TemperatureSeries}" XAxes="{Binding XAxes}" YAxes="{Binding YAxes}" Title="{Binding PTTitle}"/>
            </TabItem>
            <TabItem Header="Title" HeaderStringFormat="{}{0}3">
                <lvc:CartesianChart Grid.Row="1" Series="{Binding GassRationSeries}" Title="{Binding XTitle}"/>
            </TabItem>
            <TabItem Header="Title" HeaderStringFormat="{}{0}4">
                <lvc:CartesianChart Grid.Row="1" Grid.Column="1" Series="{Binding predictionSeries}" Title="{Binding PredictTitle}"/>
            </TabItem>
        </TabControl>
        
        
    </Grid>
</UserControl>
